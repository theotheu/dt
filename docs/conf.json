{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "../client",
            "../server"
        ],
        "exclude": [
            "../client/javascripts/lib",
            "../server/node_modules"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./html",
        "recurse": true
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    }
}